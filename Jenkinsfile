pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building World'
            }
        }
        stage('deploy') {
            steps {
                echo 'Deploying World'
            }
        }
        stage('tesing') {
            steps {
                echo 'Testing World'
            }
        }
        stage('release') {
            steps {
                echo 'Releasing World'
            }
        }
    }
}
